{
    "name": "Stackoverfrog",
    "version": "1.0",
    "manifest_version": 2,
    "browser_action": {
        "default_icon": { "128":"images/icon.png" }
    },
    "web_accessible_resources": [
        "images/*"
    ],
    "content_scripts": [
        {
            "matches": [ "https://stackoverflow.com/*" ],
            "js": [ "background.js" ]
        }
    ],
    "permissions": [ "activeTab" ]
}